{
  "name": "Teemo 💕",
  "description": "Quickly generate chat messages with embedded emoji! 💖😆",
  "version": "2.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {},
  "permissions": ["system.display"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+E",
        "windows": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Open the text editing window. 📝"
    }
  }
}
