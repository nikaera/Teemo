{
  "name": "teemo",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.1",
    "emoji-mart": "^3.0.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.3.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && cp background.js assets/* build && zip -r build.zip build/",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prettier:fix": "prettier . \"!test*\" --check --write",
    "check-types": "tsc --noEmit",
    "lint": "npm-run-all eslint check-types",
    "format": "npm-run-all lint eslint:fix prettier:fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/chrome": "0.0.145",
    "@types/emoji-mart": "^3.0.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.14",
    "@types/react": "^17.0.9",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "commitlint": "^12.1.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-app": "^6.2.2",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.3",
    "semantic-release-cli": "^5.4.3",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikaera/Teemo.git"
  }
}
